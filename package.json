{
    "name": "loginformreact",
    "version": "1.0.0",
    "description": "Login/Signup flow using MERN Stack",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "server-dev": "cd server && npm run dev",
        "client-dev": "cd client && npm start",
